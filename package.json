{
  "name": "mobile-app-distribution",
  "private": true,
  "version": "0.1.0",
  "description": "Mobile App distribution made simple",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf server/dist",
    "build": "nest build",
    "format": "prettier --write \"server/src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register server/src/main.ts",
    "dev": "nodemon",
    "prod": "node server/dist/main.js",
    "lint": "eslint \"server/{src}/**/*.ts\" --fix",
    "clean": "rimraf builds/ios/*.{ipa,plist} && rimraf builds/android/*.apk && rimraf tmp/**/* && rimraf public/icons/*.png"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.2.3",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-fastify": "^6.11.8",
    "@nestjs/serve-static": "^1.0.2",
    "@nestjs/swagger": "^4.2.11",
    "aws-sdk": "^2.624.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "fastify-static": "^2.6.0",
    "fastify-swagger": "^2.5.0",
    "hbs": "^4.1.0",
    "mime": "^2.4.4",
    "mongoose": "^5.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nest-access-control": "^2.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/mime": "^2.0.1",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "mv": "^2.1.1",
    "node-stream-zip": "^1.9.1",
    "nodemon": "^2.0.2",
    "plist": "^3.0.1",
    "pngdefry": "git+https://github.com/igez/node-pngdefry.git",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
